@model IEnumerable<RestaurantFinder.Models.Restaurant>

@{
    ViewBag.Title = "Index";
}





<div class="row">
    <div class="col-12">
        <div class="card-box">
            <h4 class="header-title">All Resturant </h4>
            <p class="sub-header">
              
            </p>
            <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>AddressLine1</th>
                        <th>AddressLine12</th>
                        <th>Area</th>
                        <th>PinCode</th>
                        <th>PinCode</th>

                       
                    </tr>
                </thead>

                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Name</td>
                            <td>@item.AddressLine1</td>
                            <td>@item.AddressLine2</td>
                            <td>@item.Area</td>
                            <td>@item.PinCode</td>
                            <td>@item.State</td>
                        </tr>

                    }




                    </tbody>
            </table>
        </div>
    </div>
</div> <!-- end row -->






@section scripts
{

    <script>
        $(document).ready(function () {
            $('#myTable').DataTable();
        });</script>}